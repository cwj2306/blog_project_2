	<h1>Fetch API 사용하기</h1>
	
	<script>
		fetch("http://localhost:8000/blog/test2",{
			method:'POST',
			headers:{
				'content-type':'text/plain'	
			}			
		}).then(function(data){
			return data.text();
		}).then()
	</script>

